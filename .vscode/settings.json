{
  "latex-workshop.latex.recipes": [
    {
      "name": "lualatex",
      "tools": [
        "lualatex-tool"
      ]
    },
  ],
 "latex-workshop.latex.tools": [
   {
     "name": "lualatex-tool",
     "command": "lualatex",
     "args": ["%DOC%"]
   }
 ],
 "latex-workshop.chktex.enabled": false,
